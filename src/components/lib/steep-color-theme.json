{
  "name": "Steep color theme",
  "type": "dark",
  "tokenColors": [
    {
      "scope": "entity.name",
      "settings": {
        "foreground": "#e16eaa"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#90cdf4"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#6c758d"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#e9ecef"
      }
    },
    {
      "scope": [
        "keyword.control.flow.block-scalar.folded.yaml",
        "storage.modifier.chomping-indicator.yaml"
      ],
      "settings": {
        "foreground": "#adb5bd"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#68d391",
        "fontStyle": "italic"
      }
    }
  ]
}
